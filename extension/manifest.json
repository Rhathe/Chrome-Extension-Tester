{
	"name": "Test Popup Extension",
	"manifest_version": 2,
	"description": "Popup Chrome Extension.",
	"version": "1.0.0",
	"minimum_chrome_version": "29",
	"web_accessible_resources": [
		"fonts/*.ttf",
		"fonts/*svg",
		"fonts/*.eot",
		"fonts/*.woff"
	],
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "Testing",
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"content_scripts": [],
	"permissions": [
		"tabs",
		"declarativeContent",
		"identity",
		"storage"
	]
}
